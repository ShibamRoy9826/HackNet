{
  "expo": {
    "name": "HackNet",
    "slug": "HackNet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "hacknet",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shibamroy.hacknet"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon/foreground.png",
        "backgroundColor": "#17171d"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.shibamroy.hacknet"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#17171d"
        }
      ],
      "expo-web-browser",
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/fonts/PhantomSans-Regular.otf"
          ],
          "android": {
            "fonts": [
              {
                "fontFamily": "PhantomSans",
                "fontDefinitions": [
                  {
                    "path": "./src/assets/fonts/PhantomSans-Regular.otf",
                    "weight": 400,
                    "style": "normal"
                  },
                  {
                    "path": "./src/assets/fonts/PhantomSans-Italic.otf",
                    "weight": 400,
                    "style": "italic"
                  },
                  {
                    "path": "./src/assets/fonts/PhantomSans-Bold.otf",
                    "weight": 700
                  }
                ]
              }
            ]
          },
          "ios": {
            "fonts": [
              "./src/assets/fonts/PhantomSans-Regular.otf",
              "./src/assets/fonts/PhantomSans-Bold.otf",
              "./src/assets/fonts/PhantomSans-Italic.otf"
            ]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "14749a4f-8461-4113-8c2b-238775dfce1c"
      }
    }
  }
}